<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.generated.code.repository.SysDictMapper">
	<resultMap id="SysDictResultMap" type="SysDict">
		<result column="dict_id" property="dictId" />
		<result column="parent_id" property="parentId" />
		<result column="dict_code" property="dictCode" />
		<result column="dict_name" property="dictName" />
		<result column="sorted" property="sorted" />
	</resultMap>

	<sql id="all_column">
		dict_id, parent_id, dict_code, dict_name, sorted
	</sql>

	<sql id="limit">
		<if test="limit != null">
			limit
			<if test="offset != null">
			#{offset},
			</if>
			#{limit}
		</if>
	</sql>

	<insert id="insert" parameterType="SysDict">
		insert into 
		sys_dict(dict_id, parent_id, dict_code, dict_name, sorted) 
		values(#{dictId}, #{parentId}, #{dictCode}, #{dictName}, #{sorted})
	</insert>

	<delete id="deleteByPrimaryKey" parameterType="int">
		delete from sys_dict 
		where dict_id = #{id}
	</delete>

	<update id="updateByPrimaryKey" parameterType="SysDict">
		update sys_dict
		<set>
 			<if test="dictId != null"> 
				dict_id = #{dictId},
			</if>
			<if test="parentId != null"> 
				parent_id = #{parentId},
			</if>
			<if test="dictCode != null"> 
				dict_code = #{dictCode},
			</if>
			<if test="dictName != null"> 
				dict_name = #{dictName},
			</if>
			<if test="sorted != null"> 
				sorted = #{sorted},
			</if>
		</set> 
		where dict_id = #{dictId}
	</update>

	<select id="selectByPrimaryKey" resultMap="SysDictResultMap" parameterType="int">
		select dict_id, parent_id, dict_code, dict_name, sorted 
		from sys_dict 
		where dict_id = #{id}
	</select>

	<!-- START 写在START和END中间的代码不会被替换 -->

	<!-- END 写在START和END中间的代码不会被替换-->

</mapper>